#include <stdio.h>
int main() {
    float num1, num2, result;
    printf("Enter dividend: ");
    scanf("%f", &num1);
    printf("Enter divisor: ");
    scanf("%f", &num2);
 if (num2 == 0) {
        printf("Error! Division by zero.\n");
    } else {
        result = num1 / num2;
        printf("%.2f / %.2f = %.2f\n", num1, num2, result);
    }
 return 0;
}
